{
  "name": "@ohxyz/combobox",
  "version": "1.1.6",
  "description": "A combo box implementation in React",
  "repository": "git@github.com:ohxyz/combobox.git",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.config.js --open --env.NODE_ENV=dev --env.COMPONENT=combobox",
    "babel": "rm -rf lib | babel ./src --out-dir ./lib --presets=react --ignore=__tests__",
    "less": "lessc ./less/components/combobox.less ./style/default.css",
    "build": "npm run babel | npm run less",
    "jest": "jest"
  },
  "main": "index.js",
  "author": "Ohxyz",
  "license": "MIT",
  "keywords": [
    "combo box",
    "autocomplete",
    "dropdown"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
